[default]
  <% @config['default'].each do |k,v| -%>
  <% if v.kind_of?(Array) -%>
  <% v.each do |subv| -%>
  <%= k %> = <%= subv %>
  <% end -%>
  <%= k %> = <%= v %>
  <% end -%>
  <% end -%>

[chef]
  <% @config['chef'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[supermarket]
  <% @config['supermarket'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[berksapi]
  <% @config['berksapi'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[tests]
  <% @config['tests'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[graphite]
  <% @config['graphite'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

<% unless @config['github'].nil? -%>
<% @config['github'].each do |k,v| -%>
[github "<%= k %>"]
  <% v.each do |subk,subv| -%>
  <%= subk %> = <%= subv %>
  <% end -%>
<% end -%>
<% end -%>

<% unless @config['chef']['enterprisechef'] == false || @config['customers'].nil? -%>
<% @config['customers'].each do |k,v| -%>
[customer "<%= k %>"]
  <% v.each do |subk,subv| -%>
  <%= subk %> = <%= subv %>
  <% end -%>
<% end -%>
<% end -%>
